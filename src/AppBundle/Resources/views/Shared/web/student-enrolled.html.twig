{% extends 'base.html.twig' %}
{% block body %}
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        {% for student in students %}
        <tr>
            <td>{{ student.id }}</td>
            <td>{{ student.person.name }}</td>
            <td>{{ student.person.surname }}</td>
            <td>{{ student.person.gender }}</td>
        </tr>
        {% endfor %}
    </tbody>

</table>
{% endblock %}
